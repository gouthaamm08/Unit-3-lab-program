#include <stdio.h>
#include <string.h>
struct Student
{
    int id;
    char name[20];
    float marks;
};
int main()
{
    struct Student s1;
    struct Student *ptr;
    ptr = &s1;
    ptr->id = 201;
    strcpy(ptr->name, "Rahul");
    ptr->marks = 92.5;
    printf("ID    : %d\n", ptr->id);
    printf("Name  : %s\n", ptr->name);
    printf("Marks : %.2f\n", ptr->marks);
    return 0;
}


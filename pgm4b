#include <stdio.h>
#include <stdlib.h>
int main()
{
    int *ptr, i, n = 5;
    ptr = (int *)calloc(n, sizeof(int));
    if (ptr == NULL)
    {
        printf("Memory allocation failed\n");
        return 1;
    }
    printf("Values stored in allocated memory:\n");
    for (i = 0; i < n; i++)
    {
        printf("%d ", ptr[i]);
    }
    free(ptr);

    return 0;
}

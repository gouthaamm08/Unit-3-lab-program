#include <stdio.h>
#include <string.h>
typedef struct
{
    int id;
    char name[20];
    float marks;
} Student;
int main()
{
    Student s1;
    s1.id = 101;
    strcpy(s1.name, "Goutham");
    s1.marks = 88.5;
    printf("Student ID   : %d\n", s1.id);
    printf("Student Name : %s\n", s1.name);
    printf("Marks        : %.2f\n", s1.marks);
    return 0;
}

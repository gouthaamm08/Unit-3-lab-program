#include <stdio.h>
#include <string.h>

struct Student
{
    int id;
    char name[20];
    float marks;
};
void display(struct Student *s)
{
    printf("Student ID   : %d\n", s->id);
    printf("Student Name : %s\n", s->name);
    printf("Marks        : %.2f\n", s->marks);
}
int main()
{
    struct Student s1;
    s1.id = 101;
    strcpy(s1.name, "Goutham");
    s1.marks = 90.5;
    display(&s1);

    return 0;
}
